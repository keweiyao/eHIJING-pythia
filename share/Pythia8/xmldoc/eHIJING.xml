</parm> 

<parm name="eHIJING:Kfactor" default="1.0" min="0.0"> 
</parm> 

<parm name="eHIJING:xG-n" default="4.0" min="1.0" max="10.0"> 
</parm> 

<parm name="eHIJING:xG-lambda" default="-0.5" min="-0.99" max="0.0"> 
</parm> 

<word name="eHIJING:TablePath" default="./eHIJING-Tables/"> 
